<!-- Form purchased_product start -->
<%= form_with(model: @purchased_product, local: true) do |f| %>
<!-- Form purchased_product end -->

  <!-- Label and input for products_id start -->
  <div class="form-group">
    <%= f.label :products_id %>

    <!-- Input for products_id start -->
    <%= f.text_field :products_id, class: "form-control" %>
    <!-- Input for products_id end -->
  </div>
  <!-- Label and input for products_id end -->

  <!-- Label and input for coupons_id start -->
  <!-- Input for coupons_id start -->
  <%= f.hidden_field :coupons_id, value: @coupon.id %>
  <!-- Input for coupons_id end -->
  <!-- Label and input for coupons_id end -->

  <!-- Label and input for customers_id start -->
  <div class="form-group">
    <%= f.label :customers_id %>

    <!-- Input for customers_id start -->
    <%= f.text_field :customers_id, class: "form-control" %>
    <!-- Input for customers_id end -->
  </div>
  <!-- Label and input for customers_id end -->

  <!-- Label and input for quantity start -->
  <div class="form-group">
    <%= f.label :quantity %>

    <!-- Input for quantity start -->
    <%= f.text_field :quantity, class: "form-control" %>
    <!-- Input for quantity end -->
  </div>
  <!-- Label and input for quantity end -->

  <%= f.submit "Create purchased product", class: "btn btn-block btn-outline-secondary" %>
<% end %>
